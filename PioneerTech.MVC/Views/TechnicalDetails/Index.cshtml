@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>EmployeeID</th>
        <th>UI</th>
        <th>Programming_Languages</th>
        <th>ORM_Technologies</th>
        <th>Databases</th>

    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td>@Model.Rows[i][0]</td>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][4]</td>
            <td>
                <a href="@Url.Action("Edit", "TechnicalDetails", new {@id=@Model.Rows[i][0] })">Edit</a>
                <a href="@Url.Action("Delete", "TechnicalDetails", new {@id=@Model.Rows[i][0] })">Delete</a>
            </td>
        </tr>
    }
</table>
<a href="@Url.Action("Create","TechnicalDetails")">Add TechnicalDetails</a>

